<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.estimotelib">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application android:label="@string/app_name">

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@android:drawable/ic_popup_reminder" />
        <meta-data
            android:name="firebase_messaging_auto_init_enabled"
            android:value="true" />

        <service
            android:name="com.estimotelib.fcm.CustomFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name="com.estimotelib.fcm.CustomFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
    </application>
</manifest>
